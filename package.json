{
  "name": "kleep-coding-game",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db-setup": "npm run db-up && npm run db-deploy && npm run db-seed",
    "db-seed": "prisma db seed",
    "db-up": "docker compose -f prisma/docker-compose.yml up -d",
    "db-deploy": "prisma migrate deploy",
    "db-migrate": "yarn prisma migrate dev",
    "generate-schemas": "prisma generate && prisma format",
    "db-reset": "npm run db-nuke && npm run db-setup",
    "db-nuke": "docker compose -f prisma/docker-compose.yml down --volumes --remove-orphans"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "dotenv": "^16.3.1",
    "next": "14.0.3",
    "prisma": "^5.6.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20.10.2",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
